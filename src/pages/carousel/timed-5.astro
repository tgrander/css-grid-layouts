---
import "@styles/carousel/timed-5.css";
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Image Carousel</title>
  </head>

  <body>
    <div class="carousel-container">
      <div class="carousel">
        <div class="carousel-slide">
          <img
            src="https://images.unsplash.com/photo-1723053140058-7aa2e74ac794?q=80&w=2970&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D"
            alt="Image 1"
          />
        </div>
        <div class="carousel-slide">
          <img
            src="https://images.unsplash.com/photo-1722525276311-67568beb03c7?q=80&w=3088&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D"
            alt="Image 2"
          />
        </div>
        <div class="carousel-slide">
          <img
            src="https://images.unsplash.com/photo-1723053145751-9aacd7c47df1?q=80&w=3088&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D"
            alt="Image 3"
          />
        </div>
        <div class="carousel-slide">
          <img
            src="https://via.placeholder.com/800x400?text=Image+4"
            alt="Image 4"
          />
        </div>
      </div>
      <button class="carousel-button prev">&lt;</button>
      <button class="carousel-button next">&gt;</button>
      <div class="carousel-dots"></div>
    </div>
    <script src="../../scripts/carousel/timed-5.ts"></script>
  </body>
</html>
